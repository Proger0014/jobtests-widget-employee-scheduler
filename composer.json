{
  "name": "proger0014/jobtest-widget-scheduler",
  "description": "Widget Scheduler extension for yii2",
  "keywords": ["yii2", "widget", "jobtests"],
  "type": "yii2-extension",
  "autoload": {
    "psr-4": {
      "proger0014\\yii2\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "yiisoft/yii2-bootstrap": "^2.0",
    "yiisoft/yii2": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "extra": {
    "bootstrap": "src\\WidgetBootstrap"
  }
}
